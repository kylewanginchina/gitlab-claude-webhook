{
  "name": "gitlab-claude-webhook",
  "version": "1.0.0",
  "description": "GitLab webhook service for Claude Code CLI integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check \"src/**/*.{ts,js}\"",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "gitlab",
    "webhook",
    "claude",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.47",
    "@gitbeaker/node": "^35.8.1",
    "@openai/codex-sdk": "^0.104.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "simple-git": "^3.20.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
